{
  "name": "joinmodule",
  "version": "1.0.0",
  "description": "Autoload your script with Typescript support.",
  "type": "module",
  "main": "dist/server.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "node --loader ts-node/esm --enable-source-maps bin/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JefteCosta/joinmodule.git"
  },
  "keywords": [
    "autoload",
    "typescript",
    "npm-module",
    "esnext",
    "consign"
  ],
  "author": "JefteCosta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JefteCosta/joinmodule/issues"
  },
  "homepage": "https://github.com/JefteCosta/joinmodule#readme",
  "devDependencies": {
    "@japa/api-client": "^2.0.3",
    "@japa/browser-client": "^2.0.3",
    "@japa/expect": "^3.0.0-1",
    "@japa/expect-type": "^2.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.0.0-9",
    "@japa/snapshot": "^2.0.5",
    "@swc/core": "^1.7.23",
    "@types/node": "^22.5.3",
    "playwright": "^1.46.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "imports": {
    "#interfaces/*": "./src/interfaces/*.js",
    "#models/*": "./src/models/*.js",
    "#services/*": "./src/services/*.js",
    "#utils/*": "./src/utils/*.js",
    "#src/*": "./src/*.js",
    "#managers/*": "./src/managers/*.js",
    "#servers/*": "./src/servers/*.js"
  }
}
